{
	"name": "my-hotkeys",
	"description": "Keyboard shorcuts",
	"version": "0.0.2",
	"license": "MIT",
	"main": "./src/index.ts",
	"module": "./dist/esm/hotkeys.esm.js",
	"browser": "./dist/browser/hotkeys.min.js",
	"types": "./dist/esm/hotkeys.esm.d.ts",
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"dev": "npm run test:watch",
		"play": "rollup --config rollup.playground.config.js --watch --bundleConfigAsCjs",
		"prebuild": "rm -rf ./dist",
		"build": "rollup --config rollup.prod.config.js --bundleConfigAsCjs",
		"postbuild": "rm -rf ./dist/esm/temp-dts",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"lint": "npm run check:lint",
		"checks": "npm run check:all",
		"types": "npm run check:types",
		"check:lint": "eslint ./src ./tests",
		"check:types": "tsc --noEmit",
		"check:all": "npm run check:lint && npm run check:types && npm test",
		"preversion": "npm run check:all",
		"version": "npm run build",
		"postversion": "git push && git push --tags",
		"prepublishOnly": "npm run check:all"
	},
	"devDependencies": {
		"@rollup/plugin-terser": "0.4.4",
		"@rollup/plugin-typescript": "12.1.2",
		"@types/jsdom": "21.1.7",
		"@typescript-eslint/eslint-plugin": "8.24.1",
		"@typescript-eslint/parser": "8.24.1",
		"@vitest/coverage-v8": "3.0.5",
		"eslint": "8.57.0",
		"jsdom": "26.0.0",
		"keyboard-simulator": "0.0.8",
		"rollup": "4.34.8",
		"rollup-plugin-dts": "6.1.1",
		"tslib": "2.8.1",
		"typescript": "5.7.3",
		"vitest": "3.0.5"
	},
	"homepage": "https://github.com/taitulism/my-hotkeys#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/taitulism/my-hotkeys.git"
	},
	"author": {
		"name": "Itai Tenenbaum",
		"email": "taitu.dev@gmail.com",
		"url": "https://github.com/taitulism"
	},
	"keywords": [
		"hotkeys",
		"keyboard",
		"hotkey",
		"shortcuts",
		"keypress",
		"keys",
		"bind",
		"combinations",
		"combination",
		"combos",
		"keyboard-event",
		"keyboard-events",
		"keybindings",
		"keydown",
		"keyup",
		"key",
		"bindings",
		"shortcut",
		"keystrokes",
		"keybinding"
	]
}
